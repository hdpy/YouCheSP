/* pages/personal/coupon.wxss */
.coupon{min-height: 100vh;background-color: #f8f8f8;}
.coupon-null{height: 30vh;background-color: #fff;text-align: center;padding-bottom: 100rpx}
.coupon-btn{text-align: center;height: 80rpx;line-height: 80rpx;position: absolute;top: 30vh;width: 100%;z-index: 100}
.coupon-btn text{margin: 0 auto;padding: 0 20rpx;border-radius: 40rpx;font-size: 28rpx;border: 1rpx solid #9596af;color: #9596af}
.coupon-null icon{font-size: 200rpx;line-height: 30vh;color: #9596af}